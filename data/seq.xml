<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <data noupdate="1">
        <!-- sequence master data ini tes -->
        <record id="seq_ini_tes" model="ir.sequence">
            <field name="name">Sequence Ini Coba</field>
            <field name="code">ini.tes.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_red" model="ir.sequence">
            <field name="name">Sequence releted</field>
            <field name="code">ini.relet.sequence</field>
            <field name="prefix">HA</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>